    "@ueno/tslint-config": "1.0.5",
    "autoprefixer": "9.4.10",
    "classnames": "2.2.6",
    "cross-env": "5.2.0",
    "next-classnames-loader": "1.0.3",
    "node-sass": "4.11.0",
    "nodemon": "1.18.10",
    "postcss-csso": "3.0.0",
    "stylelint": "9.10.1",
    "svg-react-loader": "0.4.6",
    "ts-node": "8.0.2",
    "tslint": "5.13.1",
    "tslint-react": "3.6.0",
    "typescript": "3.3.3333"
