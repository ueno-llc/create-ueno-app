    "gsap-tools": "1.0.9",
    "lodash": "4.17.11",
    "next": "8.1.0",
    "next-compose-plugins": "2.2.0",
    "next-fonts": "0.17.0",
    "next-images": "1.1.1",
    "next-react-svg": "1.0.0",
    "next-videos": "1.1.0",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-helmet": "5.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.3.0",
